{
  "name": "ui-ui-color-palette",
  "version": "0.1.0",
  "description": "",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "npm-run-all -p start:announcements_worker start:auth_lobby start:dev",
    "start:announcements_worker": "cd ../announcements-yelbolt-worker && npm run start:8888",
    "start:auth_lobby": "cd ../auth-yelbolt && npm run ui-color-palette",
    "start:figmaui3:dark": "UI_THEME=\"figma-ui3\" COLOR_MODE=\"figma-dark\" npm run build & vite preview",
    "start:figmaui3:light": "UI_THEME=\"figma-ui3\" COLOR_MODE=\"figma-light\" npm run build & vite preview",
    "start:figmaui2:dark": "UI_THEME=\"figma-ui2\" COLOR_MODE=\"figma-dark\" npm run build & vite preview",
    "start:figmaui2:light": "UI_THEME=\"figma-ui2\" COLOR_MODE=\"figma-light\" npm run build & vite preview",
    "start:penpot:dark": "UI_THEME=\"penpot\" COLOR_MODE=\"dark\" npm run build & vite preview",
    "start:penpot:light": "UI_THEME=\"penpot\" COLOR_MODE=\"light\" npm run build & vite preview",
    "build": "npx vite build --mode development",
    "build:prod": "npx vite build --mode production",
    "typecheck": "npx tsc --noEmit",
    "typecheck:watch": "npx tsc --noEmit --watch",
    "lint": "npx eslint ./src/** --fix --ext .ts,.tsx .",
    "lint:watch": "esw ./src/** --fix --watch --ext .ts,.tsx .",
    "format": "npx prettier './src' --write",
    "update:figmug": "npm i @a_ng_d/figmug-ui@latest @a_ng_d/figmug-utils@latest",
    "update:ui-color-palette": "npm i @a_ng_d/utils-ui-color-palette@latest"
  },
  "keywords": [
    "color",
    "color-scheme",
    "lch",
    "color-system",
    "ui-color",
    "penpot-plugin"
  ],
  "author": "Aur√©lien Grimaud",
  "licence": "MIT",
  "repository": "https://github.com/a-ng-d/ui-ui-color-palette",
  "peerDependencies": {
    "preact": "^10.6.4",
    "react": "npm:@preact/compat",
    "react-dom": "npm:@preact/compat"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.10.1",
    "@sentry/vite-plugin": "^3.3.1",
    "@types/chroma-js": "^3.1.1",
    "@types/file-saver": "^2.0.5",
    "@types/jszip": "^3.4.1",
    "@types/node": "^22.14.0",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-basic-ssl": "^2.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitejs/plugin-react-swc": "^3.8.1",
    "chroma-js": "^3.1.2",
    "css-loader": "^6.5.1",
    "eslint": "^8.57.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.25.3",
    "eslint-watch": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "prettier-plugin-css-order": "^2.1.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "typescript": "^5.4.3",
    "vite": "^6.2.5",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-sentry": "^1.4.1",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "dependencies": {
    "@a_ng_d/figmug-ui": "^1.8.16",
    "@a_ng_d/figmug-utils": "^0.3.0",
    "@a_ng_d/utils-ui-color-palette": "^1.0.6",
    "@nanostores/preact": "^0.5.2",
    "@sentry/react": "^8.8.0",
    "@supabase/supabase-js": "^2.44.2",
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "mixpanel-figma": "^2.0.4",
    "uid": "^2.0.2"
  }
}
