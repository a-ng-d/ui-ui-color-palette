{
  "name": "ui-ui-color-palette",
  "version": "1.2.5",
  "private": true,
  "description": "",
  "type": "module",
  "workspaces": [
    "packages/announcements-yelbolt-worker",
    "packages/auth-yelbolt-worker",
    "packages/auth-yelbolt"
  ],
  "scripts": {
    "install:packages": "npm install --workspaces",
    "update:packages": "git submodule update --remote --merge",
    "update:figmug": "npm i @a_ng_d/figmug-ui@latest @a_ng_d/figmug-utils@latest",
    "update:uicp": "npm i @a_ng_d/utils-ui-color-palette@latest",
    "start:announcements": "npm run start:8888 -w announcements-yelbolt-worker",
    "start:auth": "npm run ui-color-palette -w auth-yelbolt",
    "start:packages": "npm run start:announcements & npm run start:auth",
    "start:figma:dark:figma": "PLATFORM=figma COLOR_MODE=dark EDITOR=figma vite --mode development",
    "start:figma:light:figma": "PLATFORM=figma COLOR_MODE=light EDITOR=figma vite --mode development",
    "start:figma:dark:dev": "PLATFORM=figma COLOR_MODE=dark EDITOR=dev vite --mode development",
    "start:figma:light:dev": "PLATFORM=figma COLOR_MODE=light EDITOR=dev vite --mode development",
    "start:penpot:dark:penpot": "PLATFORM=penpot COLOR_MODE=dark EDITOR=penpot vite --mode development",
    "start:penpot:light:penpot": "PLATFORM=penpot COLOR_MODE=light EDITOR=penpot vite --mode development",
    "typecheck": "npx tsc --noEmit",
    "typecheck:watch": "npx tsc --noEmit --watch",
    "lint": "npx eslint ./src/** --fix --ext .ts,.tsx .",
    "lint:watch": "esw ./src/** --fix --watch --ext .ts,.tsx .",
    "format": "npx prettier './src' --write"
  },
  "author": "Aur√©lien Grimaud",
  "licence": "MIT",
  "repository": "https://github.com/a-ng-d/ui-ui-color-palette",
  "devDependencies": {
    "@preact/preset-vite": "^2.10.1",
    "@sentry/vite-plugin": "^3.3.1",
    "@types/chroma-js": "^3.1.1",
    "@types/file-saver": "^2.0.5",
    "@types/jszip": "^3.4.1",
    "@types/node": "^22.14.0",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-basic-ssl": "^2.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitejs/plugin-react-swc": "^3.8.1",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.25.3",
    "prettier": "^3.2.5",
    "prettier-plugin-css-order": "^2.1.2",
    "typescript": "^5.4.3",
    "vite": "^6.2.5",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-sentry": "^1.4.1",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "dependencies": {
    "@a_ng_d/figmug-ui": "^1.12.4",
    "@a_ng_d/figmug-utils": "^0.4.0",
    "@a_ng_d/utils-ui-color-palette": "^1.3.20",
    "@nanostores/preact": "^0.5.2",
    "@sentry/react": "^9.27.0",
    "@supabase/supabase-js": "^2.44.2",
    "chroma-js": "^3.1.2",
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "mixpanel-figma": "^2.0.4",
    "preact": "^10.6.4",
    "react": "npm:@preact/compat",
    "react-dom": "npm:@preact/compat",
    "uid": "^2.0.2"
  }
}
